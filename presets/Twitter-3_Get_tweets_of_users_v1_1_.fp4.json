{
  "name": "3 Get tweets of users (v1.1)",
  "category": "Twitter",
  "description": "Note. The free pre-registered accesses no longer work. Fetching Twitter data with Facepager is only possible after setting up a paid v2 access (https://developer.twitter.com/en).",
  "module": "Twitter",
  "options": {
    "auth": "disable",
    "auth_prefix": "",
    "auth_tokenname": "",
    "auth_type": "OAuth1",
    "auth_uri": "",
    "basepath": "https://api.twitter.com/1.1",
    "extension": ".json",
    "key_paging": "next_cursor_str",
    "nodedata": null,
    "objectid": "id",
    "pages": 1,
    "paging_stop": "next_cursor",
    "paging_type": "key",
    "param_paging": "cursor",
    "params": {
      "count": "200",
      "include_rts": "1",
      "user_id": "<Object ID>"
    },
    "redirect_uri": "",
    "resource": "/statuses/user_timeline",
    "token_uri": ""
  },
  "speed": 200,
  "saveheaders": false,
  "timeout": 15,
  "maxsize": 5,
  "columns": [
    "created_at",
    "text",
    "user.screen_name",
    "user.id_str",
    "retweet_count",
    "favorite_count",
    "retweeted_status.id",
    "quoted_status.id",
    "in_reply_to_status_id_str",
    "in_reply_to_user_id",
    "entities.user_mentions.*.id_str",
    "entities.hashtags.*.text",
    "entities.urls.*.url",
    "entities.symbols"
  ]
}