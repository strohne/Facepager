{
  "name": "OpenAlex: Get list of works by search term",
  "category": "Bibliometrics",
  "description": "Get list of works by search term as seed node. For example get all works, \"computational methods\" in the title, abstract, or fulltext. \n\nThis preset provides 100 titels, for more increase per-page parameter. Results are sorted by publication year. You can also sort alphabetically or by relevance score for example. For more information have a look at: https://docs.openalex.org/how-to-use-the-api/get-lists-of-entities/sort-entity-lists \n",
  "module": "Generic",
  "options": {
    "basepath": "https://api.openalex.org",
    "resource": "/works",
    "params": {
      "search": "<Object ID>",
      "per-page": "100",
      "sort": "publication_year"
    },
    "extension": "",
    "headers": {},
    "verb": "GET",
    "format": "json",
    "filename": "<None>",
    "fileext": "<None>",
    "pages": 1,
    "paging_type": "key",
    "key_paging": null,
    "paging_stop": null,
    "param_paging": null,
    "nodedata": "results",
    "objectid": null,
    "scope": "",
    "proxy": "",
    "auth_type": "Disable",
    "auth_uri": "",
    "redirect_uri": "",
    "token_uri": "",
    "auth": "disable",
    "auth_tokenname": "",
    "auth_prefix": ""
  },
  "speed": 200,
  "saveheaders": false,
  "timeout": 15,
  "maxsize": 5,
